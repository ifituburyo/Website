<svg width="800" height="600" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="grad1" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#667eea;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#764ba2;stop-opacity:1" />
    </linearGradient>
  </defs>
  <rect width="800" height="600" fill="url(#grad1)"/>
  <g transform="translate(400,300)">
    <!-- 3D Chip representation -->
    <rect x="-150" y="-100" width="300" height="200" fill="#1a1a1a" rx="20"/>
    <rect x="-140" y="-90" width="280" height="180" fill="#2c2c2c" rx="15"/>
    
    <!-- Pins -->
    <rect x="-180" y="-80" width="30" height="10" fill="#silver"/>
    <rect x="-180" y="-50" width="30" height="10" fill="#silver"/>
    <rect x="-180" y="-20" width="30" height="10" fill="#silver"/>
    <rect x="-180" y="10" width="30" height="10" fill="#silver"/>
    <rect x="-180" y="40" width="30" height="10" fill="#silver"/>
    <rect x="-180" y="70" width="30" height="10" fill="#silver"/>
    
    <rect x="150" y="-80" width="30" height="10" fill="#silver"/>
    <rect x="150" y="-50" width="30" height="10" fill="#silver"/>
    <rect x="150" y="-20" width="30" height="10" fill="#silver"/>
    <rect x="150" y="10" width="30" height="10" fill="#silver"/>
    <rect x="150" y="40" width="30" height="10" fill="#silver"/>
    <rect x="150" y="70" width="30" height="10" fill="#silver"/>
    
    <!-- Center circle with glow -->
    <circle cx="0" cy="0" r="40" fill="#667eea" opacity="0.3"/>
    <circle cx="0" cy="0" r="30" fill="#667eea" opacity="0.5"/>
    <circle cx="0" cy="0" r="20" fill="#667eea"/>
    
    <!-- Text -->
    <text x="0" y="150" font-family="Arial, sans-serif" font-size="24" fill="white" text-anchor="middle" font-weight="bold">Electronic Component</text>
  </g>
</svg>
